{"lineNum":"  751","line":"\t\t\tprefix="},
{"lineNum":"  752","line":"\t\t\t# skip all squash! or fixup! (but save for later)"},
{"lineNum":"  753","line":"\t\t\twhile :"},
{"lineNum":"  754","line":"\t\t\tdo"},
{"lineNum":"  755","line":"\t\t\t\tcase \"$rest\" in"},
{"lineNum":"  756","line":"\t\t\t\t\"squash! \"*|\"fixup! \"*)"},
{"lineNum":"  757","line":"\t\t\t\t\tprefix=\"$prefix${rest%%!*},\""},
{"lineNum":"  758","line":"\t\t\t\t\trest=\"${rest#*! }\""},
{"lineNum":"  759","line":"\t\t\t\t\t;;"},
{"lineNum":"  760","line":"\t\t\t\t*)"},
{"lineNum":"  761","line":"\t\t\t\t\tbreak"},
{"lineNum":"  762","line":"\t\t\t\t\t;;"},
{"lineNum":"  763","line":"\t\t\t\tesac"},
{"lineNum":"  764","line":"\t\t\tdone"},
{"lineNum":"  765","line":"\t\t\tprintf \'%s %s %s %s\\n\' \"$sha1\" \"$action\" \"$prefix\" \"$rest\""},
{"lineNum":"  766","line":"\t\t\t# if it\'s a single word, try to resolve to a full sha1 and"},
{"lineNum":"  767","line":"\t\t\t# emit a second copy. This allows us to match on both message"},
{"lineNum":"  768","line":"\t\t\t# and on sha1 prefix"},
{"lineNum":"  769","line":"\t\t\tif test \"${rest#* }\" = \"$rest\"; then"},
{"lineNum":"  770","line":"\t\t\t\tfullsha=\"$(git rev-parse -q --verify \"$rest\" 2>/dev/null)\""},
{"lineNum":"  771","line":"\t\t\t\tif test -n \"$fullsha\"; then"},
{"lineNum":"  772","line":"\t\t\t\t\t# prefix the action to uniquely identify this line as"},
{"lineNum":"  773","line":"\t\t\t\t\t# intended for full sha1 match"},
{"lineNum":"  774","line":"\t\t\t\t\techo \"$sha1 +$action $prefix $fullsha\""},
{"lineNum":"  775","line":"\t\t\t\tfi"},
{"lineNum":"  776","line":"\t\t\tfi"},
{"lineNum":"  777","line":"\t\tesac"},
{"lineNum":"  778","line":"\tdone >\"$1.sq\" <\"$1\""},
{"lineNum":"  779","line":"\ttest -s \"$1.sq\" || return"},
{"lineNum":"  780","line":""},
{"lineNum":"  781","line":"\tused="},
{"lineNum":"  782","line":"\twhile read -r pick sha1 message"},
{"lineNum":"  783","line":"\tdo"},
{"lineNum":"  784","line":"\t\tcase \" $used\" in"},
{"lineNum":"  785","line":"\t\t*\" $sha1 \"*) continue ;;"},
{"lineNum":"  786","line":"\t\tesac"},
