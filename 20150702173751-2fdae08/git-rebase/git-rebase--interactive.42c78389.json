{"lineNum":"  882","line":"\t\t\t\tdie \"Could not commit staged changes.\""},
{"lineNum":"  883","line":"\t\telse"},
{"lineNum":"  884","line":"\t\t\tdo_with_author git commit --no-verify -F \"$msg\" -e \\"},
{"lineNum":"  885","line":"\t\t\t\t${gpg_sign_opt:+\"$gpg_sign_opt\"} ||"},
{"lineNum":"  886","line":"\t\t\t\tdie \"Could not commit staged changes.\""},
{"lineNum":"  887","line":"\t\tfi"},
