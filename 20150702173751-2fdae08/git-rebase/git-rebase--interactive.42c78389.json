{"lineNum":"  297","line":"\tfi"},
{"lineNum":"  298","line":""},
{"lineNum":"  299","line":"\techo $sha1 >> \"$state_dir\"/current-commit"},
{"lineNum":"  300","line":""},
{"lineNum":"  301","line":"\t# rewrite parents; if none were rewritten, we can fast-forward."},
{"lineNum":"  302","line":"\tnew_parents="},
