{"lineNum":"  788","line":"\t\tused=\"$used$sha1 \""},
{"lineNum":"  789","line":"\t\twhile read -r squash action msg_prefix msg_content"},
{"lineNum":"  790","line":"\t\tdo"},
{"lineNum":"  791","line":"\t\t\tcase \" $used\" in"},
